@functions {
    string GetValidationBg(GestionProjects.Domain.Enums.StatutValidationCloture statut)
    {
        return statut switch
        {
            GestionProjects.Domain.Enums.StatutValidationCloture.Validee => "#d1fae5",
            GestionProjects.Domain.Enums.StatutValidationCloture.Rejetee => "#fee2e2",
            _ => "#fef3c7"
        };
    }

    string GetValidationColor(GestionProjects.Domain.Enums.StatutValidationCloture statut)
    {
        return statut switch
